<html>
<head>
    <title>@ViewData["Title"] - Luke Varley</title>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1, user-scalable=no" />
    <link rel="stylesheet" href="~/css/html5up/main.css" />
    <link rel="stylesheet" href="~/css/site.min.css" asp-append-version="true" />
    <noscript><link rel="stylesheet" href="~/css/html5up/noscript.css" /></noscript>
</head>
<body class="is-loading">
    <!-- Wrapper -->
    <div id="wrapper" class="fade-in">

        @RenderBody()


        <!-- Copyright -->
        <div id="copyright">
            <ul><li>&copy; Luke Varley</li><li>Design: <a href="https://html5up.net">HTML5 UP</a></li></ul>
        </div>
    </div>

    <!-- Scripts -->
    <script src="~/js/html5up/jquery.min.js"></script>
    <script src="~/js/html5up/jquery.scrollex.min.js"></script>
    <script src="~/js/html5up/jquery.scrolly.min.js"></script>
    <script src="~/js/html5up/skel.min.js"></script>
    <script src="~/js/html5up/util.js"></script>
    <script src="~/js/html5up/main.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
</body>
</html>














































